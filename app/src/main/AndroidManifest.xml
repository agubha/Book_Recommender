<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.a.book_recommender">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainPage">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".PopUp.Sign_up_pop_up"
            android:theme="@style/pop_up_theme" />
        <activity
            android:name=".PopUp.Sign_in_pop_up"
            android:theme="@style/pop_up_theme" />
        <activity android:name=".HomePage" />
        <activity android:name=".SearchBooks" />
        <activity android:name=".AdminAdd" />
        <activity android:name=".DisplaySimilarBooks"></activity>
    </application>

</manifest>